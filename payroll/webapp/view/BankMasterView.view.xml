<mvc:View xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" controllerName="com.app.payroll.controller.BankMasterView" 
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Bank Master" showNavButton="true" navButtonPress="onNavBackPress">
		<List items="{path:'HouseBankAccounts'}">
			<items>
				<ObjectListItem title="{HouseBankAccounts>BankCode}" intro="{HouseBankAccounts>Branch}" number="{HouseBankAccounts>ISRType}" type="Active" press="onListItemPress">
					<firstStatus>
						<ObjectStatus text="{HouseBankAccounts>State}" state="Success"/>
					</firstStatus>
					<ObjectAttribute/>
					<ObjectAttribute text="{HouseBankAccounts>Country}"/>
				</ObjectListItem>
			</items>
		</List>
	</Page>
</mvc:View>