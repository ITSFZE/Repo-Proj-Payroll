{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.app.payroll",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"config": {"sapFiori2Adaptation": true},
		"rootView": {
			"viewName": "com.app.payroll.view.App",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.app.payroll.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.app.payroll.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "Dashboard",
				"pattern": "",
				"target": "Dashboard"
			},
			{
				"name": "JobMaster",
				"pattern": "RouteJobMaster",
				"target": ["JobMaster"]
			}, {
				"name": "EditJobMaster",
				"pattern": "EditJobMaster",
				"target": ["EditJobMaster"]
			}, {
				"name": "CreateJob",
				"pattern": "CreateJob",
				"target": ["CreateJob"]
			}, {
				"name": "Login",
				"pattern": "Login",
				"titleTarget": "",
				"greedy": false,
				"target": ["Login"]
			}],

			"targets": {
				"Dashboard": {
					"viewId": "Dashboard",
					"viewName": "Dashboard",
					"viewLevel" : 1
				},
				"Login": {
					"viewType": "XML",
					"viewName": "Login",
					"viewId": "app"
				},
				"JobMaster": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "JobMaster"
				},
				"EditJobMaster": {
					"viewType": "XML",
					"viewName": "EditJobMaster",
					"viewLevel": 2
				},
				"CreateJob": {
					"viewType": "XML",
					"viewName": "CreateJob",
					"viewLevel": 3
				}
			}
		}
	}
}