{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.app.payroll",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"config": {
			"sapFiori2Adaptation": true
		},
		"rootView": {
			"viewName": "com.app.payroll.view.App",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.app.payroll.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.app.payroll.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "Login",
					"pattern": "",
					"target": [
						"Login"
					]
				},
				{
					"name": "Dashboard",
					"pattern": "Dashboard",
					"target": [
						"Dashboard"
					]
				},
				{
					"name": "JobMaster",
					"pattern": "JobMaster",
					"target": [
						"JobMaster"
					]
				},
				{
					"name": "EditJobMaster",
					"pattern": "EditJobMaster",
					"target": [
						"EditJobMaster"
					]
				},
				{
					"name": "CreateJob",
					"pattern": "CreateJob",
					"target": [
						"CreateJob"
					]
				},
				{
					"name": "EmployeeGroup",
					"pattern": "EmployeeGroup",
					"target": [
						"EmployeeGroup"
					]
				},
				{
					"name": "UpdateEmployeeGroup",
					"pattern": "UpdateEmployeeGroup",
					"target": [
						"UpdateEmployeeGroup"
					]
				},
				{
					"name": "CreateEmployeeGroup",
					"pattern": "CreateEmployeeGroup",
					"target": [
						"CreateEmployeeGroup"
					]
				},
				{
					"name": "WageMaster",
					"pattern": "WageMaster",
					"target": [
						"WageMasterDetail",
						"WageMaster"
					]
				},
				{
					"name": "WageMasterDetail",
					"pattern": "WageMasterDetail",
					"target": "WageMasterDetail"
				},
				{
					"name": "EditWageMasterDetail",
					"pattern": "EditWageMasterDetail/{id}",
					"target": [
						"WageMasterDetail",
						"WageMaster"
					]
				},
				{
					"name": "BankMaster",
					"pattern": "BankMaster",
					"target": [
						"BankMasterDetail",
						"BankMasterView"
					]
				},
				{
					"name": "EditBankMaster",
					"pattern": "EditBankMaster/{Code}",
					"target": [
						"EditBankMasterDetail",
						"BankMasterView"
					]
				}
			],
			"targets": {
				"SplitContainerMasterAndDetail": {
					"viewName": "SplitContainerMasterAndDetail"
				},
				"UnifiedSplitContainer": {
					"viewName": "UnifiedSplitContainer"
				},
				"Login": {
					"viewName": "Login",
					"viewLevel": 1
				},
				"Dashboard": {
					"viewName": "Dashboard",
					"viewLevel": 2
				},
				"JobMaster": {
					"viewName": "JobMaster",
					"parent": "UnifiedSplitContainer",
					"controlId": "UnifiedSplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 3
				},
				"EditJobMaster": {
					"viewName": "EditJobMaster",
					"parent": "UnifiedSplitContainer",
					"controlId": "UnifiedSplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 4
				},
				"CreateJob": {
					"viewName": "CreateJob",
					"parent": "UnifiedSplitContainer",
					"controlId": "UnifiedSplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 5
				},
				"EmployeeGroup": {
					"viewName": "EmployeeGroup",
					"parent": "UnifiedSplitContainer",
					"controlId": "UnifiedSplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 6
				},
				"UpdateEmployeeGroup": {
					"viewName": "UpdateEmployeeGroup",
					"parent": "UnifiedSplitContainer",
					"controlId": "UnifiedSplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 7
				},
				"CreateEmployeeGroup": {
					"viewName": "CreateEmployeeGroup",
					"parent": "UnifiedSplitContainer",
					"controlId": "UnifiedSplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 8
				},
				"WageMaster": {
					"viewName": "WageMaster",
					"parent": "SplitContainerMasterAndDetail",
					"controlId": "SplitContainer",
					"controlAggregation": "masterPages",
					"viewLevel": 9
				},
				"WageMasterDetail": {
					"viewName": "WageMasterDetail",
					"parent": "SplitContainerMasterAndDetail",
					"controlId": "SplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 10
				},
				"EditWageMasterDetail": {
					"viewName": "WageMasterDetail",
					"parent": "SplitContainerMasterAndDetail",
					"controlId": "SplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 11
				},
				"BankMasterView": {
					"viewName": "BankMasterView",
					"parent": "SplitContainerMasterAndDetail",
					"controlId": "SplitContainer",
					"controlAggregation": "masterPages",
					"viewLevel": 12
				},
				"BankMasterDetail": {
					"viewName": "BankMasterDetail",
					"parent": "SplitContainerMasterAndDetail",
					"controlId": "SplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 13
				},
				"EditBankMasterDetail": {
					"viewName": "EditBankMasterDetail",
					"parent": "SplitContainerMasterAndDetail",
					"controlId": "SplitContainer",
					"controlAggregation": "detailPages",
					"viewLevel": 14
				}
			}
		}
	}
}